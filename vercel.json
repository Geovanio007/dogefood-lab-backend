{
  "version": 2,
  "buildCommand": "cd frontend && yarn install --legacy-peer-deps && yarn build",
  "outputDirectory": "frontend/build",
  "routes": [
    {
      "src": "/game-mechanisms.html",
      "dest": "/game-mechanisms.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|webp|woff|woff2|ttf|eot|json|map))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
